<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Data</title>
	<link rel="stylesheet" type="text/css" href="style.css">
</head>
<body>
    <h1>User Data</h1>
    <table class="dataTable">
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
			<th>Priority</th>
            <th>Subject</th>
			<th>Description</th>
        </tr>
        <% data.forEach(user => { %>
            <tr>
                <td><%= user.name %></td>
                <td><%= user.email %></td>
                <td><%= user.phno %></td>
				<td><%= user.priority %></td>
				<td><%= user.subject %></td>
                <td><%= user.description %></td>
            </tr>
        <% }); %>
    </table>
</body>
</html>
